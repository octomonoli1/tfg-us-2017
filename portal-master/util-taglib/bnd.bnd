Bundle-Name: ${manifest.bundle.name}
Bundle-SymbolicName: ${manifest.bundle.symbolic.name}
Bundle-Version: 2.4.1
Export-Package:\
	com.liferay.alloy.taglib.*;version="2.0.0",\
	com.liferay.taglib.*
Import-Package:\
	!com.liferay.portal.freemarker,\
	\
	!jodd.*,\
	\
	!org.dom4j.*,\
	\
	!org.json.*,\
	\
	com.ibm.crypto.provider;resolution:=optional,\
	\
	com.liferay.*,\
