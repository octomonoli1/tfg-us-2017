<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_0_0.dtd">

<routes>
	<route>
		<pattern>/rss/{id}</pattern>
		<ignored-parameter name="p_p_cacheability" />
		<implicit-parameter name="p_p_lifecycle">2</implicit-parameter>
		<implicit-parameter name="p_p_resource_id">rss</implicit-parameter>
		<overridden-parameter name="p_p_cacheability">cacheLevelFull</overridden-parameter>
	</route>
	<route>
		<pattern>/rss/{groupId}/{feedId}</pattern>
		<ignored-parameter name="p_p_cacheability" />
		<implicit-parameter name="p_p_lifecycle">2</implicit-parameter>
		<implicit-parameter name="p_p_resource_id">rss</implicit-parameter>
		<overridden-parameter name="p_p_cacheability">cacheLevelFull</overridden-parameter>
	</route>
</routes>