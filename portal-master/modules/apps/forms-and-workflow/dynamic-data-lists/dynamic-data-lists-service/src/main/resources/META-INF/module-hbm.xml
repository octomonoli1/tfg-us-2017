		<property name="lastPublishDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="com.liferay.dynamic.data.lists.model.impl.DDLRecordVersionImpl" table="DDLRecordVersion">
		<id name="recordVersionId" type="long">
