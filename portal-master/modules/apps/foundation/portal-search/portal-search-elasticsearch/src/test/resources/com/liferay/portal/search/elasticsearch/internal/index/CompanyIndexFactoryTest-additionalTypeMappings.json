{
	"dynamic_templates": [
		{
			"template_ja": {
				"mapping": {
					"analyzer": "kuromoji_liferay_custom",
					"index": "analyzed",
					"store": "true",
					"term_vector": "with_positions_offsets",
					"type": "string"
				},
				"match": "\\w+_ja\\b|\\w+_ja_[A-Z]{2}\\b",
				"match_mapping_type": "string",
				"match_pattern": "regex"
			}
		}
	]
}