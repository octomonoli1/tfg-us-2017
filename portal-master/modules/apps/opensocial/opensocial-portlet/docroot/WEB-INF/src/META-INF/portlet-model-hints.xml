<?xml version="1.0"?>

<model-hints>
	<model name="com.liferay.opensocial.model.Gadget">
		<field name="uuid" type="String" />
		<field name="gadgetId" type="long" />
		<field name="companyId" type="long" />
		<field name="createDate" type="Date" />
		<field name="modifiedDate" type="Date" />
		<field name="name" type="String" />
		<field name="url" type="String">
			<hint-collection name="URL" />
		</field>
		<field name="portletCategoryNames" type="String">
			<hint name="max-length">4000</hint>
		</field>
		<field name="lastPublishDate" type="Date" />
	</model>
	<model name="com.liferay.opensocial.model.OAuthConsumer">
		<field name="oAuthConsumerId" type="long" />
		<field name="companyId" type="long" />
		<field name="createDate" type="Date" />
		<field name="modifiedDate" type="Date" />
		<field name="gadgetKey" type="String" />
		<field name="serviceName" type="String" />
		<field name="consumerKey" type="String" />
		<field name="consumerSecret" type="String">
			<hint-collection name="CLOB" />
		</field>
		<field name="keyType" type="String" />
	</model>
	<model name="com.liferay.opensocial.model.OAuthToken">
		<field name="oAuthTokenId" type="long" />
		<field name="companyId" type="long" />
		<field name="userId" type="long" />
		<field name="userName" type="String" />
		<field name="createDate" type="Date" />
		<field name="modifiedDate" type="Date" />
		<field name="gadgetKey" type="String" />
		<field name="serviceName" type="String" />
		<field name="moduleId" type="long" />
		<field name="accessToken" type="String" />
		<field name="tokenName" type="String" />
		<field name="tokenSecret" type="String" />
		<field name="sessionHandle" type="String" />
		<field name="expiration" type="long" />
	</model>
</model-hints>