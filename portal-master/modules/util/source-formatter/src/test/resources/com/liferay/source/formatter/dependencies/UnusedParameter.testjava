package com.liferay.source.formatter.dependencies;

/**
 * @author Hugo Huijser
 */
public class UnusedParameter {

	public UnusedParameter(int length, int width, int height, String color) {
		_volume = _getVolume(length, width, height, color);
	}

	private int _getVolume(int length, int width, int height, String color) {
		return length * width * height;
	}

	private final int _volume;

}